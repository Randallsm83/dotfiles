[user]
	name = Randall Miller
	email = randallsm83@gmail.com
[core]
	editor = vim
	pager = diff-so-fancy | less --tabs=4 -RFX
[color]
	ui = true
	diff = auto
[init]
	defaultBranch = master
[pull]
	rebase = true
[push]
	default = simple
	autoSetupRemote = true
[grep]
	lineNumber = true
[color "diff-highlight"]
	oldNormal = 167 bold
	oldHighlight = 167 reverse
	newNormal = 142 bold
	newHighlight = 142 reverse
[color "diff"]
	meta = 214 bold
	frag = 175 bold
	commit = 208 bold
	old = 167 bold
	new = 142 bold
	whitespace = 167 reverse
[diff-so-fancy]
	first-run = false
[interactive]
	diffFilter = diff-so-fancy --patch
